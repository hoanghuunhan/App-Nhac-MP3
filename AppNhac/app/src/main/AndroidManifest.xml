<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="huunhan.hn.com.appnhac">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>

    <application
        android:name=".Service.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppNhac"
        android:usesCleartextTraffic="true">

        <activity
            android:name=".Activity.PlayNhacActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.DanhsachAlbumActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.DanhsachtheloaitheochudeActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.DanhsachcacchudeActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.DanhsachcacplaylistActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.DanhsachbaihatActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".Service.MyService"/>
        <receiver android:name=".Service.MyReceiver"/>
    </application>

</manifest>